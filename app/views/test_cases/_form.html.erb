<div class="form-wrapper">
  <%= form_for @test_case, url: contest_test_cases_path(@contest) do |f| %>
    <%= f.text_field :input, class: 'form-control', placeholder: 'Input (optional)' %>
    <br />

    <%= f.text_field :expected_output, class: 'form-control', placeholder: 'Expected output' %>
    <br />

    <%= f.submit "Add test case", class: 'btn btn-default form-control' %>
  <% end %>
</div>
